{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error", {
            "endOfLine": "auto"
        }],
        "prefer-destructuring": "off",
        "import/prefer-default-export":" off"
    }
}
